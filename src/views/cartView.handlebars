<main>
  <div>
    {{#if emptyCart}}
      <h1>Empty Cart</h1>
    {{else}}
      {{#each products}}
        <ul>
          <li>
            <h3>Producto: {{product.title}}</h3>
            <p>Costo: {{product.price}}</p>
            <p>Cantidad: {{qty}}</p>
          </li>
        </ul>
      {{/each}}
      <p>Total de compra: {{total}}</p>

      <button disabled>Comprar</button>
    {{/if}}
  </div>
</main>